{
  "name": "i18n-helper",
  "displayName": "i18n-helper",
  "license": "MIT",
  "description": "国际化文案助手",
  "repository": {
    "url": "https://github.com/Allen3039/i18nherlper.git"
  },
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.34.0"
  },
  "publisher": "allen3039",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld",
    "onCommand:nima",
    "onCommand:i18n",
    "onCommand:i18n.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "nima",
        "title": "nnnnn"
      },
      {
        "command": "i18n",
        "title": "i18n"
      },
      {
        "command": "i18n.search",
        "title": "i18n.search"
      }
    ],
    "configuration": {
      "title": "i18n configuration",
      "properties": {
        "18n.localePath": {
          "type": "string",
          "description": "本地翻译的字典文件地址",
          "default": "./xx.js",
          "scope": "resource"
        }
      }
    },
    "keybindings": [
      {
        "command": "i18n",
        "key": "alt+shift+x",
        "mac": "alt+shift+x"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.135",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
